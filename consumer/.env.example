# gRPC Server Configuration
GRPC_PORT=50051

# Web Server Configuration
WEB_PORT=3000

# Consumer Configuration
NUM_CONSUMERS=4
QUEUE_MAX_SIZE=10

# File Storage
UPLOAD_DIR=./uploaded-videos

# Video Processing
# FFmpeg path (leave empty to use system FFmpeg)
FFMPEG_PATH=
# Preview duration in seconds
PREVIEW_DURATION=10

# Dead Letter Queue Configuration
DLQ_MAX_RETRIES=3
DLQ_INITIAL_DELAY_MS=1000

# Video Compression
COMPRESSION_ENABLED=true
COMPRESSION_CODEC=libx264
COMPRESSION_CRF=23
COMPRESSION_PRESET=ultrafast
COMPRESSION_AUDIO_BITRATE=128k
