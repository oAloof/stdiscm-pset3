{
  "name": "media-upload-consumer",
  "version": "1.0.0",
  "description": "Consumer service for media upload system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && mkdir -p dist/proto && cp ../proto/*.proto dist/proto/",
    "start": "node dist/consumer/src/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "serve": "npm run build && npm start"
  },
  "keywords": [
    "grpc",
    "consumer",
    "video-upload",
    "queue"
  ],
  "author": "oAloof",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.10",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "fluent-ffmpeg": "^2.1.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/uuid": "^9.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}